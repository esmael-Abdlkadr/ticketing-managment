{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  {
    "scripts": {
      "dev": "ts-node-dev --respawn --transpile-only server.ts",
      "start": "node dist/server.js",
      "build": "tsc",
      "build:prod": "tsc && (pnpm sentry:sourcemaps || echo 'Skipping Sentry sourcemaps')",
      "sentry-wizard": "sentry-wizard -i sourcemaps --saas",
      "sentry:sourcemaps": "npx sentry-cli sourcemaps inject --org anonshare --project anonshare-api ./dist && npx sentry-cli sourcemaps upload --org anonshare --project anonshare-api ./dist"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sentry/node": "^9.3.0",
    "@sentry/profiling-node": "^9.3.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/ejs": "^3.1.5",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.13.8",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.17",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.0",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.10",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.11.0",
    "multer": "1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.10.0",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.2",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@sentry/cli": "^2.42.2"
  }
}
